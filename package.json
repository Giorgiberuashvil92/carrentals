{
  "name": "grt-website-b2b",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr-staging": "npm run build:client-and-server-bundles-staging && npm run compile:server",
    "build:ssr-production": "npm run build:client-and-server-bundles-production && npm run compile:server",
    "build:client-and-server-bundles-staging": "ng build --configuration=staging --prod --outputHashing=all && ng run grt-website-b2b:server:staging",
    "build:client-and-server-bundles-production": "ng build --configuration=production --prod --outputHashing=all && ng run grt-website-b2b:server:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.5",
    "@angular/cdk": "^10.2.5",
